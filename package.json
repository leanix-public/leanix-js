{
  "name": "leanix-js",
  "version": "0.0.1-beta.1",
  "description": "Javascript client library for LeanIX",
  "main": "dist/index.js",
  "types": "./dist/index.d.js",
  "scripts": {
    "setup": "npm install",
    "build": "tsc -p ./tsconfig.json",
    "clean": "shx rm -rf node_modules/ dist/ docs/ leanix-js-*.tgz",
    "tslint": "tslint --fix -c tslint.json 'src/**/*.ts'",
    "test-only": "mocha --exit -r ts-node/register src/**/*.spec.ts",
    "pretest": "npm run build",
    "test": "npm run test-only",
    "typedoc": "typedoc --out ./docs --mode modules --tsconfig ./tsconfig.json ./src",
    "posttypedoc": "shx cp .nojekyll docs/.nojekyll"
  },
  "author": "Paulo dos Santos <paulo.santos@leanix.net>",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/jwt-decode": "^2.2.1",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.0.0",
    "@types/request-promise-native": "^1.0.14",
    "@types/uuid": "^3.4.3",
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "shx": "^0.2.2",
    "ts-node": "^6.0.1",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.5.3",
    "uuid": "^3.2.1"
  },
  "dependencies": {
    "jwt-decode": "^2.2.0",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5"
  }
}
